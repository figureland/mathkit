{
  "name": "@figureland/mathkit",
  "version": "0.0.4",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.js"
    },
    "./vector2": {
      "types": "./dist/vector2.d.ts",
      "require": "./dist/vector2.js",
      "import": "./dist/vector2.js"
    },
    "./matrix2d": {
      "types": "./dist/matrix2D.d.ts",
      "require": "./dist/matrix2D.js",
      "import": "./dist/matrix2D.js"
    },
    "./style": {
      "types": "./dist/style.d.ts",
      "require": "./dist/style.js",
      "import": "./dist/style.js"
    }
  },
  "description": "A minimal toolkit for working with numbers, matrices and vectors",
  "scripts": {
    "build": "bun run build.ts",
    "prepublishOnly": "bun run build",
    "prepare": "husky",
    "lint": "eslint src/",
    "format": "prettier --write src/",
    "release": "bunx changelogen@latest --release"
  },
  "files": [
    "dist"
  ],
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^19.2.1",
    "@commitlint/config-conventional": "^19.1.0",
    "@types/bun": "^1.0.0",
    "bun-plugin-dts": "^0.2.1",
    "eslint": "^9.0.0",
    "husky": "^9.0.11",
    "prettier": "^3.2.5",
    "typescript": "^5.2.2",
    "typescript-eslint": "^7.6.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/figureland/mathkit.git"
  },
  "author": "Figure Systems",
  "bugs": {
    "url": "https://github.com/figureland/mathkit"
  },
  "homepage": "https://github.com/figureland/mathkit#readme"
}